{
  "name": "alexa-global-thermonuclear-war",
  "version": "1.0.0",
  "description": "Alexa plays Global Thermonuclear War",
  "main": "src/index.js",
  "scripts": {
    "lint": "jshint src/*.js",
    "pretest": "npm run lint",
    "test": "mocha test/ --compilers js:babel-core/register",
    "test:watch": "npm run test -- -w",
    "prebuild": "npm run test",
    "build": "webpack",
    "prezip": "npm run build",
    "zip": "zip -vj alexa-global-thermonuclear-war.zip dist/index.js",
    "preupload": "npm run zip",
    "upload": "aws lambda update-function-code --zip-file fileb://alexa-global-thermonuclear-war.zip --function-name alexaGlobalThermonuclearWar"
  },
  "author": "Robin Norwood <robin.norwood@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "assert": "^1.4.1",
    "babel": "^6.23.0",
    "babel-core": "^6.20.0",
    "babel-loader": "^6.2.9",
    "babel-preset-es2015": "^6.18.0",
    "jshint": "^2.9.4",
    "json-loader": "^0.5.4",
    "mocha": "^3.2.0",
    "webpack": "^1.14.0"
  },
  "dependencies": {
    "alexa-sdk": "^1.0.7"
  }
}
